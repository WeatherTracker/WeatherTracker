<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>

            <!-- 实心 -->
            <solid android:color="#98CAD1"/>

            <!--邊框顏色、粗細-->
            <stroke
                android:width="6dp"
                android:color="#332688"
                android:dashWidth="8dp"
                android:dashGap="6dp"/>

            <!--圓角-->
            <corners android:radius="20dp" />

        </shape>
    </item>
</selector>